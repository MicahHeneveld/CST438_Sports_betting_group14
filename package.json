{
  "name": "gambleapp",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--no-experimental-strip-types expo start",
    "start:clear": "cross-env NODE_OPTIONS=--no-experimental-strip-types expo start --clear",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "demo": "node demo-presentation/presentation-controller.js",
    "demo:auto": "npx playwright test demo-presentation/demo.spec.js --project=chromium --headed",
    "demo:signup": "npx playwright test e2e-tests/live-signup-demo.spec.js --project=chromium --headed",
    "demo:signup-full": "npx playwright test e2e-tests/full-signup-demo.spec.js --project=chromium --headed",
    "demo:interactive": "npx playwright test demo-presentation/demo.spec.js --project=chromium --headed --grep \"Interactive\"",
    "demo:screenshots": "npx playwright test demo-presentation/demo.spec.js --project=chromium --reporter=null",
    "e2e": "npx playwright test e2e-tests/ --headed",
    "e2e:headless": "npx playwright test e2e-tests/"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@react-navigation/stack": "^7.1.1",
    "better-sqlite3": "^11.8.1",
    "cors": "^2.8.5",
    "expo": "^54.0.12",
    "expo-blur": "~15.0.7",
    "expo-constants": "~18.0.9",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.10",
    "expo-splash-screen": "~31.0.10",
    "expo-sqlite": "~16.0.8",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.8",
    "express": "^4.21.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "^0.5.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@playwright/test": "^1.56.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.1.10",
    "@types/react-test-renderer": "^18.3.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "jest": "^29.2.1",
    "jest-expo": "~54.0.12",
    "react-test-renderer": "^19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
